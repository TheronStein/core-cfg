{
  "Console log": {
    "prefix": "cl",
    "body": ["console.log(${1:message})"],
    "description": "Console log"
  },
  "Console error": {
    "prefix": "ce",
    "body": ["console.error(${1:message})"],
    "description": "Console error"
  },
  "Arrow function": {
    "prefix": "af",
    "body": ["const ${1:name} = (${2:params}) => {", "\t${0:// body}", "}"],
    "description": "Arrow function"
  },
  "Async arrow function": {
    "prefix": "aaf",
    "body": ["const ${1:name} = async (${2:params}) => {", "\t${0:// body}", "}"],
    "description": "Async arrow function"
  },
  "Try catch": {
    "prefix": "tc",
    "body": [
      "try {",
      "\t${1:// code}",
      "} catch (${2:error}) {",
      "\t${0:// handle error}",
      "}"
    ],
    "description": "Try catch block"
  },
  "Import": {
    "prefix": "imp",
    "body": ["import ${2:module} from '${1:path}'"],
    "description": "Import statement"
  },
  "Export default": {
    "prefix": "expd",
    "body": ["export default ${1:name}"],
    "description": "Export default"
  }
}

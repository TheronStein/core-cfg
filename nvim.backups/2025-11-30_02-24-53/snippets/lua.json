{
  "Print statement": {
    "prefix": "pr",
    "body": ["print(${1:message})"],
    "description": "Quick print statement"
  },
  "Print inspect": {
    "prefix": "pi",
    "body": ["print(vim.inspect(${1:variable}))"],
    "description": "Print inspected table"
  },
  "Function": {
    "prefix": "fn",
    "body": [
      "function ${1:name}(${2:args})",
      "\t${0:-- body}",
      "end"
    ],
    "description": "Function definition"
  },
  "Local function": {
    "prefix": "lfn",
    "body": [
      "local function ${1:name}(${2:args})",
      "\t${0:-- body}",
      "end"
    ],
    "description": "Local function definition"
  },
  "For loop": {
    "prefix": "for",
    "body": [
      "for ${1:i} = ${2:1}, ${3:10} do",
      "\t${0:-- body}",
      "end"
    ],
    "description": "Numeric for loop"
  },
  "For pairs": {
    "prefix": "forp",
    "body": [
      "for ${1:key}, ${2:value} in pairs(${3:table}) do",
      "\t${0:-- body}",
      "end"
    ],
    "description": "Pairs iterator"
  },
  "If statement": {
    "prefix": "if",
    "body": [
      "if ${1:condition} then",
      "\t${0:-- body}",
      "end"
    ],
    "description": "If statement"
  }
}

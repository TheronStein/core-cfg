function lskb() {
   local -A keyb=(); for k v in ${(kv)Zkeymaps}; do
     k="%F{1}%B$k%f%b" v="%F{3}$v%f" keyb[${(%)k}]=${(%)v}
   done
   print -raC 2 -- ${(Oakv)keyb[@]}

 # local -a keys
